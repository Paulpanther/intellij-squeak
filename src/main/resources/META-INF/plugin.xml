<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>com.paulpanther.intellijsqueak</id>
    <name>Squeak</name>
    <vendor>paulpanther</vendor>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileType
                name="Smalltalk File"
                implementationClass="com.paulpanther.intellijsqueak.SmalltalkFileType"
                language="Smalltalk"
                extensions="st"/>
        <moduleType
            id="SMALLTALK_MODULE_TYPE"
            implementationClass="com.paulpanther.intellijsqueak.SmalltalkModuleType"/>
    </extensions>
</idea-plugin>
